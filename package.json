{
  "name": "gym-management-system",
  "version": "1.0.0",
  "description": "A comprehensive Gym Management System with member billing, notifications, and receipts",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "test": "jest --runInBand --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint backend/**/*.js",
    "format": "prettier --write \"backend/**/*.js\"",
    "db:init": "node database/init.js",
    "migrate": "node database/migrate.js"
  },
  "keywords": [
    "gym",
    "management",
    "billing",
    "members",
    "receipts",
    "notifications"
  ],
  "author": "Gym Management Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "sqlite": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "dotenv": "^16.0.3",
    "cors": "^2.8.5",
    "express-validator": "^7.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.13.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "jest": "^29.4.0",
    "supertest": "^6.3.3",
    "eslint": "^8.33.0",
    "prettier": "^2.8.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
